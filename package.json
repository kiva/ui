{
	"name": "kvue",
	"version": "1.0.0",
	"description": "The Kiva ui",
	"author": "braincrave",
	"private": true,
	"license": "UNLICENSED",
	"scripts": {
		"dev": "cross-env NODE_ENV=development node --max-old-space-size=2048 server/dev-server.js",
		"start": "cross-env NODE_ENV=production node --max-old-space-size=2048 --http-server-default-timeout=240000 server/index.js",
		"inspect-dev": "cross-env NODE_ENV=development node --inspect --max-old-space-size=4096 server/dev-server.js",
		"inspect": "cross-env NODE_ENV=production node --inspect --max-old-space-size=4096 server/index.js",
		"build": "rimraf dist && node build/build.js",
		"build:trace": "rimraf dist && node --trace-deprecation build/build.js",
		"build:all": "rimraf dist && npm run build:client && npm run build:server",
		"build:client": "rimraf dist/static && webpack --config build/webpack.client.prod.conf.js --progress --hide-modules",
		"build:server": "webpack --config build/webpack.server.conf.js --progress --hide-modules",
		"destroyBuildCache": "rimraf node_modules/.cache/hard-source",
		"e2e:dev": "cypress open --config baseUrl=http://localhost:8888",
		"e2e:headless": "cypress run --config baseUrl=http://localhost:8888",
		"fetchSchema": "node build/fetch-schema.js && wait-on build/schema.graphql",
		"report": "webpack-bundle-analyzer dist/client-bundle-stats.json",
		"unit": "jest --config test/unit/jest.conf.js --coverage",
		"test": "npm run lint && npm run unit && npm run build && npm run test:e2e",
		"test:dev": "npm run dev -- --mock --config=local.vm & wait-on http-get://localhost:8888/ui-site-map && npm run e2e:dev",
		"test:e2e": "npm start -- --mock --config=local & wait-on tcp:8888 && npm run e2e:headless",
		"lint": "npm run fetchSchema && npm run lint:js && npm run lint:css && npm run lint:graphql",
		"lint:js": "eslint --ext .js,.vue --ignore-pattern 'src/graphql' src test/unit/specs",
		"lint:css": "stylelint \"src/**/*.{vue,scss}\"",
		"lint:graphql": "eslint --ext .graphql src/graphql",
		"storybook": "start-storybook -s ./public -p 6006",
		"build-storybook": "build-storybook -s ui",
		"deploy-storybook": "storybook-to-ghpages"
	},
	"husky": {
		"hooks": {
			"pre-push": "npm run lint",
			"post-checkout": "npm run destroyBuildCache"
		}
	},
