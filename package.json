{
	"name": "kvue",
	"version": "1.0.0",
	"description": "The Kiva ui",
	"author": "braincrave",
	"private": true,
	"licencse": "UNLICENCSED",
	"scripts": {
		"dev": "cross-env NODE_ENV=development node server/dev-server.js",
		"start": "cross-env NODE_ENV=production node server/index.js",
		"build": "rimraf dist && node build/build.js",
		"build:all": "rimraf dist && npm run build:client && npm run build:server",
		"build:client": "rimraf dist/static && cross-env NODE_ENV=production webpack --config build/webpack.client.prod.conf.js --progress --hide-modules",
		"build:server": "cross-env NODE_ENV=production webpack --config build/webpack.server.conf.js --progress --hide-modules",
		"unit": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js --single-run",
		"test": "npm run lint && npm run lint:css && npm run unit",
		"lint": "eslint --ext .js,.vue src test/unit/specs",
		"lint:css": "stylelint --syntax scss 'src/**/*.+(vue|scss)'"
	},
